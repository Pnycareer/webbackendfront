{
  "name": "admin-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron.mjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron": "set NODE_ENV=dev && electron .",
    "electron:prod": "npm run build && set NODE_ENV=prod && electron .",
    "dist:win": "npm run build && electron-builder --win"
  },
  "build": {
    "appId": "com.yourcompany.admin-dashboard",
    "productName": "Admin Dashboard",
    "files": [
      "dist/**",
      "electron.mjs",
      "preload.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^43.2.0",
