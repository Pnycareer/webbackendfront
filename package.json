    "jodit-react": "^4.1.2",
    "lexical": "^0.34.0",
    "lucide-react": "^0.417.0",
    "next-themes": "^0.4.6",
    "notistack": "^3.0.2",
    "npm": "^10.8.3",
    "quill": "^1.3.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2",
